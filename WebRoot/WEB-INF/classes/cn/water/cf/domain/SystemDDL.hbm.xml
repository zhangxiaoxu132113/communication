<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class table="t_systemDDL" name="cn.water.cf.domain.SystemDDL">
		<id name="seq_id" column="seq_id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="keyword" type="java.lang.String">
			<column name="keyword"/>
		</property>
		<property name="ddlCode" type="java.lang.Integer">
			<column name="ddlCode" />
		</property>
		<property name="ddlName" type="java.lang.String">
			<column name="ddlName"/>
		</property>
		<!-- <cache usage="read-write"/> -->
	</class>
	
</hibernate-mapping>